{
    'parser': 'babel-eslint',
    'rules': {
        'arrow-body-style': 0,
        'consistent-return': 0,
        'dot-notation': 0,
        'import/prefer-default-export': 0,
        'import/newline-after-import': 0,
        'import/extensions': 0,
        'new-cap': 0,
        'newline-per-chained-call': 0,
        'no-await-in-loop': 0,
        'no-console': 0,
        'no-continue': 0,
        'no-mixed-operators': 0,
        'no-param-reassign': 0,
        'no-plusplus': 0,
        'no-underscore-dangle': 0,
        'no-unused-expressions': 0,
        'no-unused-vars': [2, { "args": "none" }],
    },
    'env': {
        'jest': true,
    },
}
